		"typo3/symfony-psr-event-dispatcher-adapter": "^1.0 || ^2.0",
		"typo3fluid/fluid": "^2.7.0"
	},
	"require-dev": {
		"codeception/codeception": "^4.1.22",
		"codeception/lib-asserts": "^1.13.2",
		"codeception/module-asserts": "^1.3.1",
