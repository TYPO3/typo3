		"symfony/filesystem": "^5.3.0",
		"symfony/finder": "^5.3.0",
		"symfony/http-foundation": "^5.3.0",
		"symfony/mailer": "^5.3.0",
		"symfony/mime": "^5.3.0",
		"symfony/polyfill-intl-icu": "^1.6",
		"symfony/polyfill-intl-idn": "^1.10",
		"symfony/polyfill-intl-normalizer": "^1.22",
