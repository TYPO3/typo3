  <file source-language="en" datatype="plaintext" original="EXT:backend/Resources/Private/Language/locallang_pages_new.xlf" date="2011-10-17T20:22:37Z" product-name="backend" target-language="fa">
    <header/>
    <body>
      <trans-unit id="title" resname="title" approved="yes">
        <source>Create multiple pages</source>
        <target state="final">ساخت صفحات متعدد</target>
      </trans-unit>
      <trans-unit id="canNotCreateNew.title" resname="canNotCreateNew.title" approved="yes">
        <source>Insufficient permissions</source>
        <target state="final">مجوزهای ناکافی</target>
      </trans-unit>
      <trans-unit id="canNotCreateNew.message" resname="canNotCreateNew.message" approved="yes">
        <source>Sorry, you are not allowed to create new subpages to this page due to access restrictions.</source>
        <target state="final">متاسفیم! به دلیل محدودیت دسترسی، شما امکان ساخت زیرصفحات را در این صفحه را ندارید.</target>
      </trans-unit>
      <trans-unit id="newPage" resname="newPage" approved="yes">
        <source>Page</source>
        <target state="final">صفحه</target>
      </trans-unit>
      <trans-unit id="addMoreLines" resname="addMoreLines" approved="yes">
        <source>Add more lines</source>
        <target state="final">اضافه کردن موارد جدید</target>
      </trans-unit>
      <trans-unit id="listEnd" resname="listEnd" approved="yes">
        <source>Place new pages after the existing subpages</source>
        <target state="final">صفحات جدید را پس از زیرصفحات موجود قرار بده</target>
      </trans-unit>
      <trans-unit id="hidePages" resname="hidePages" approved="yes">
        <source>Hide new pages</source>
        <target state="final">صفحات جدید در حالت نامرئی باشند</target>
      </trans-unit>
      <trans-unit id="hidePagesInMenus" resname="hidePagesInMenus" approved="yes">
        <source>Hide new pages in menus</source>
        <target state="final">صفحات جدید در فهرست‌ها پنهان باشند</target>
      </trans-unit>
      <trans-unit id="create" resname="create" approved="yes">
        <source>Create pages</source>
        <target state="final">ساخت صفحات</target>
      </trans-unit>
      <trans-unit id="reset" resname="reset" approved="yes">
        <source>Clear fields</source>
        <target state="final">حذف بخش‌ها</target>
      </trans-unit>
      <trans-unit id="newSubPages" resname="newSubPages" approved="yes">
        <source>New page structure</source>
        <target state="final">ساختار صفحات جدید</target>
      </trans-unit>
      <trans-unit id="noPagesCreated.title" resname="noPagesCreated.title" approved="yes">
        <source>No pages added</source>
        <target state="final">صفحه‌ای اضافه نشد</target>
      </trans-unit>
      <trans-unit id="noPagesCreated.message" resname="noPagesCreated.message" approved="yes">
        <source>No pages were created.</source>
        <target state="final">هیچ صفحۀ جدیدی ایجاد نشد!</target>
      </trans-unit>
      <trans-unit id="pagesCreated.title" resname="pagesCreated.title" approved="yes">
        <source>Pages added</source>
        <target state="final">صفحه‌(هائی) اضافه شد</target>
      </trans-unit>
      <trans-unit id="pagesCreated.message" resname="pagesCreated.message" approved="yes">
        <source>The pages were created successfully.</source>
        <target state="final">صفحات مورد نظر با موفقیت ایجاد شدند.</target>
      </trans-unit>
    </body>
