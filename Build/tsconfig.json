{
    "compilerOptions": {
        "target": "es2020",
        "allowSyntheticDefaultImports": true,
        "incremental": true,
        "tsBuildInfoFile": "./.cache/tsconfig.tsbuildinfo",
        "module": "es2020",
        "moduleResolution": "node",
        "sourceMap": false,
        "removeComments": false,
        "alwaysStrict": true,
        "downlevelIteration": true,
        "experimentalDecorators": true,
        "useDefineForClassFields": false,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noImplicitReturns": true,
        "pretty": true,
        "baseUrl": "Sources/TypeScript",
        "paths": {
            "@typo3/adminpanel/*": [
                "adminpanel/*"
            ],
            "@typo3/backend/*": [
                "backend/*"
            ],
            "@typo3/belog/*": [
                "belog/*"
            ],
            "@typo3/beuser/*": [
                "beuser/*"
            ],
            "@typo3/core/*": [
                "core/*"
            ],
            "@typo3/dashboard/*": [
                "dashboard/*"
            ],
            "@typo3/extensionmanager/*": [
                "extensionmanager/*"
            ],
            "@typo3/filelist/*": [
                "filelist/*"
            ],
            "@typo3/form/*": [
                "form/*"
            ],
            "@typo3/impexp/*": [
                "impexp/*"
            ],
            "@typo3/info/*": [
                "info/*"
            ],
            "@typo3/install/*": [
                "install/*"
            ],
            "@typo3/linkvalidator/*": [
                "linkvalidator/*"
            ],
            "@typo3/lowlevel/*": [
                "lowlevel/*"
            ],
            "@typo3/opendocs/*": [
                "opendocs/*"
            ],
            "@typo3/recordlist/*": [
                "recordlist/*"
            ],
            "@typo3/recycler/*": [
                "recycler/*"
            ],
            "@typo3/redirects/*": [
                "redirects/*"
            ],
            "@typo3/rte-ckeditor/*": [
                "rte_ckeditor/*"
            ],
            "@typo3/scheduler/*": [
                "scheduler/*"
            ],
            "@typo3/setup/*": [
                "setup/*"
            ],
            "@typo3/t3editor/*": [
                "t3editor/*"
            ],
            "@typo3/tstemplate/*": [
                "tstemplate/*"
            ],
            "@typo3/viewpage/*": [
                "viewpage/*"
            ],
            "@typo3/workspaces/*": [
                "workspaces/*"
            ]
        },
        "outDir": "./JavaScript/",
        "typeRoots": [
            "node_modules/@types",
            "types"
        ]
    },
    "exclude": [
        "composer/"
    ]
}
