{
	"name": "typo3/cms-info",
	"type": "typo3-cms-framework",
	"description": "TYPO3 CMS Info - TYPO3 backend module for displaying information, such as a pagetree overview or a localization overview.",
	"homepage": "https://typo3.org",
	"license": ["GPL-2.0-or-later"],
	"authors": [{
		"name": "TYPO3 Core Team",
		"email": "typo3cms@typo3.org",
		"role": "Developer"
	}],
	"support": {
		"chat": "https://typo3.org/help",
		"docs": "https://docs.typo3.org",
		"issues": "https://forge.typo3.org",
		"source": "https://github.com/typo3/typo3"
	},
	"config": {
		"sort-packages": true
	},
	"require": {
		"typo3/cms-core": "12.0.*@dev"
	},
	"conflict": {
		"typo3/cms": "*"
	},
	"replace": {
		"typo3/cms-info-pagetsconfig": "*"
	},
	"extra": {
		"branch-alias": {
			"dev-main": "12.0.x-dev",
			"dev-master": "12.0.x-dev"
		},
		"typo3/cms": {
			"Package": {
				"partOfFactoryDefault": true
			},
			"extension-key": "info"
		}
	},
	"autoload": {
		"psr-4": {
			"TYPO3\\CMS\\Info\\": "Classes/"
		}
	}
}
