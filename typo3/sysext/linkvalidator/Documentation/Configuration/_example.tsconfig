    tt_content := addToList(mysitepackage_carousel_morelink)
    mysitepackage_carousel_carousel_items = text,typolink
  }
  linktypes = db,file,external
  checkhidden = 0
  mail {
    fromname = TYPO3 LinkValidator
    fromemail = no_reply@example.org
